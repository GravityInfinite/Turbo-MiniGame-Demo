<!--index.wxml-->
<view>
  <view class="box">
    <button type="primary" bindtap="handleRegister">注册</button>
    <button type="primary" bindtap="handleEvent">埋点事件上报</button>
    <button type="primary" bindtap="handleQueryUser">查询用户信息</button>
  </view>
  <view class="box">
    <button type="primary" bindtap="handleProfileSet">profileSet</button>
    <button type="primary" bindtap="handleProfileSetOnce">profileSetOnce</button>
    <button type="primary" bindtap="handleProfileIncrement">profileIncrement</button>
    <button type="primary" bindtap="handleProfileDelete">profileDelete</button>
    <button type="primary" bindtap="handleProfileAppend">profileAppend</button>
    <button type="primary" bindtap="handleProfileUnset">profileUnset</button>
  </view>
  <view class="box">
    <button type="primary" bindtap="handleRegisterApp">registerApp</button>
  </view>
  <view class="box">
    <button type="primary" bindtap="handleRegisterEvent">registerEvent</button>
    <button type="primary" bindtap="handleLoginEvent">loginEvent</button>
    <button type="primary" bindtap="handleLogoutEvent">logoutEvent</button>
  </view>
  <view class="box">
    <button type="primary" bindtap="handleCustomTrack">customTrack</button>
  </view>
</view>